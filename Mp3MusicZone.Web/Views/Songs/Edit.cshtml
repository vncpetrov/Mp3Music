@model SongFormModel

@{
    ViewData["Title"] = "Edit song";
}

<div class="container">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <h2 class="heading">@ViewData["Title"]</h2>
            <hr />

            <partial name="_SongForm"
                     model="@Model" />
        </div>
    </div>
</div>


@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
